{
    "name": "movies-db",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "npm-run-all --parallel dev:*",
        "static": "npm-run-all static:webpack-server-check static:webpack-client-check static:start-server-check",
        "dev:start-server": "nodemon --watch build --exec build\\server.bundle.js",
        "static:start-server-check": "node --inspect-brk build\\server.bundle.js",
        "dev:webpack-server": "webpack --config webpack.server.js --watch --mode development --open --hot --env.NODE_ENV=development",
        "static:webpack-server-check": "webpack --config webpack.server.js --mode development --env.NODE_ENV=development",
        "dev:webpack-client": "webpack --config webpack.client.js --watch --mode development --open --hot --env.NODE_ENV=development",
        "static:webpack-client-check": "webpack --config webpack.client.js --mode development --env.NODE_ENV=development",
        "build-client": "webpack --config webpack.client.js --mode production --env.NODE_ENV=production",
        "build-server": "webpack --config webpack.server.js --mode production --env.NODE_ENV=production",
        "test": "jest",
        "coverage": " jest --coverage --ci",
        "cypress": "cypress open"
    },
    "author": "Vadym Zaporozhets",
    "license": "ISC",
    "dependencies": {
        "@material-ui/core": "^3.9.2",
        "axios": "^0.18.0",
        "classnames": "^2.2.6",
        "concurrently": "^4.1.0",
        "connected-react-router": "^6.4.0",
        "express": "^4.16.4",
        "nodemon": "^1.19.1",
        "npm-run-all": "^4.1.5",
        "prop-types": "^15.7.2",
        "react": "^16.8.4",
        "react-dom": "^16.8.4",
        "react-redux": "^6.0.1",
        "react-router-config": "^5.0.0",
        "react-router-dom": "^4.4.0-beta.8",
        "react-test-renderer": "^16.8.4",
        "redux": "^4.0.1",
        "redux-devtools-extension": "^2.13.8",
        "redux-persist": "^5.10.0",
        "redux-saga": "^1.0.2",
        "reselect": "^4.0.0",
        "serialize-javascript": "^1.7.0",
        "webpack-merge": "^4.2.1"
    },
    "devDependencies": {
        "@babel/core": "^7.3.4",
        "@babel/plugin-proposal-class-properties": "^7.4.0",
        "@babel/plugin-transform-runtime": "^7.4.3",
        "@babel/polyfill": "^7.4.3",
        "@babel/preset-env": "^7.4.5",
        "@babel/preset-react": "^7.0.0",
        "babel-eslint": "^10.0.1",
        "babel-jest": "^24.5.0",
        "babel-loader": "^8.0.5",
        "cypress": "^3.2.0",
        "enzyme": "^3.9.0",
        "enzyme-adapter-react-16": "^1.11.2",
        "enzyme-to-json": "^3.3.5",
        "eslint": "^5.15.1",
        "eslint-config-prettier": "^4.1.0",
        "eslint-loader": "^2.1.2",
        "eslint-plugin-prettier": "^3.0.1",
        "eslint-plugin-react": "^7.12.4",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^3.2.0",
        "jest": "^24.5.0",
        "prettier": "^1.16.4",
        "redux-saga-test-plan": "^4.0.0-beta.2",
        "webpack": "^4.29.6",
        "webpack-cli": "^3.2.3",
        "webpack-dev-server": "^3.2.1",
        "webpack-node-externals": "^1.7.2"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.{js,jsx}": [
            "prettier --write",
            "eslint --fix",
            "git add"
        ]
    }
}
